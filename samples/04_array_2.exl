main:
        # Random range
        DATA @1000 1
        DATA @1010 100
        # Pointer start address
        DATA @1020 10
        # Data item size in bytes
        DATA @1030 8
        # Loop range
        DATA @1040 0
        DATA @1050 20
        CALL 1

generate:
        RAND @10 @1000 @1010
        LOCATE @1020
        PUSH @10 @1030
        ADD @1020 @1020 @1030
        INC @1040
        CMP @1040 @1050